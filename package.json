    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@octokit/core": "^5.1.0",
